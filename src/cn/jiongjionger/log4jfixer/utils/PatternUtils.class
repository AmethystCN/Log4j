package cn.jiongjionger.log4jfixer.utils;import java.util.regex.Matcher;import java.util.regex.Pattern;public class PatternUtils {    private static final Pattern log4jPattern = Pattern.compile("\\$\\{.*\\}");    public PatternUtils() {    }    public static boolean match(String input) {        if (input != null && !input.isEmpty()) {            Matcher matcher = log4jPattern.matcher(input);            return matcher.find();        } else {            return false;        }    }}